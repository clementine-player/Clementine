<?xml version="1.0" encoding="UTF-8"?>
<lyricproviders>
  <provider name="azlyrics.com" title="{artist} LYRICS - {title}" charset="utf-8" url="http://www.azlyrics.com/lyrics/{artist}/{title}.html">
    <urlFormat replace=" ._@,;&amp;\/()'&quot;-" with=""/>
